python-pip source: source-contains-prebuilt-windows-binary src/pip/_vendor/distlib/t64.exe
python-pip source: source-contains-prebuilt-windows-binary src/pip/_vendor/distlib/w32.exe
python-pip source: source-contains-prebuilt-windows-binary src/pip/_vendor/distlib/w64.exe
